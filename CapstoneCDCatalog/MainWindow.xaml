<Window x:Name="CDCatalogWindow" x:Class="CapstoneCDCatalog.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CapstoneCDCatalog"
        mc:Ignorable="d"
        Title="Zach's CD Catalog" Height="600" Width="800">
    <Grid>
        <Button x:Name="addButton" Content="Add" HorizontalAlignment="Left" Margin="40,525,0,0" VerticalAlignment="Top" Width="75" Click="addButton_Click"/>
        <Button x:Name="removeButton" Content="Remove" HorizontalAlignment="Left" Margin="120,525,0,0" VerticalAlignment="Top" Width="75" Click="removeButton_Click"/>

    </Grid>
</Window>
